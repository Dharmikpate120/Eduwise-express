                    <% Resources.map((item)=>{ %>
                    <li class="nav-item">
                      <a class="" style="cursor: pointer"><%= item.title%></a>
                      <ul class="navbar-nav nav mx-auto">
                        <%item.more.map((e)=>{%>
                        <li class="nav-item">
                          <a href="<%=e.link%>" class="nav-link"
                            ><%=e.title%></a
                          >
                        </li>

                        <%})%>
                      </ul>
                    </li>
                    <% })%>
                  </ul>
                </li>
                <li class="nav-item">
                  <a href="/current-affairs" class="nav-link"
                    >Current Affairs</a
                  >
                </li>
                <li class="nav-item">
                  <a href="/mentorship" class="nav-link">MentorShip</a>
                </li>
                <!-- <li class="nav-item">
