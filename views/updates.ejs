          <div class="row">
            <div class="col-sm-12 col-md-8 col-lg-8">
              <div class="intro_text">
                <h1>Updates Pate</h1>
                <div class="pages_links">
                  <a href="#" title><%=subject%></a>
                  <a href="#" title class="active"><%=content.title%></a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>
    <div
      id="mainWarningCover"
      class="disappear"
      style="
        display: flex;
        justify-content: center;
        font-size: 2rem;
        font-weight: 700;
        margin: 5rem;
        color: red;
      "
    >
      <%=content.warning%>
    </div>
    <div class="container" id="mainContentCover">
      <div class="row">
        <div class="col-md-12">
          <h1><%=subject.toUpperCase()%></h1>
          <%content.language.map(({name,resourcePdf})=>{ %>

          <div class="subsection">
            <h3 class="subsection-title">
              <%=content.title%> <%=name%>
              <a class="download_btn" download="book" href="<%=resourcePdf%>"
                >Download the PDF</a
              >
            </h3>
            <div class="object-container">
              <object
                data="../../uploads/<%=resourcePdf%>"
                type="application/pdf"
                width="100%"
                height="600"
              >
                <p>
                  Your browser does not support PDF embedding.
                  <a href="<%=name%>">Download the PDF</a>
                </p>
              </object>
            </div>
          </div>
          <%})%>
        </div>
      </div>
    </div>

