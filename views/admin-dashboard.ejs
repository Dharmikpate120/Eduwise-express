    <script>
      function setCookie(name, value, days) {
        let date = new Date();
        date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000);
        let expires = "expires=" + date.toUTCString();
        document.cookie = name + "=" + value + ";" + expires;
      }
      function getCookie(name) {
        var value = null;
        document.cookie.split(";").map((element) => {
          var arr = element.split("=");
          if (arr[0] === ` ${name}` || arr[0] === name) {
            value = arr[1] === "" ? null : arr[1];
          }
        });
        return value;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const signinWarning = document.getElementById("signin_warning");
        const signinForms = document.getElementById("admin_signin");

        authCookie = getCookie("authorization");
        if (!authCookie) {
          signinForms.classList.add("disappear");
          signinWarning.innerText = "you don't have admin access!";
        } else {
          signinWarning.classList.add("disappear");
        }

        const addUpdateForm = document.getElementById("addUpdateForm");
        const addUpdateWarning = document.getElementById("addUpdateWarning");
        addUpdateForm.addEventListener("submit", function (event) {
          event.preventDefault();
          const formData = new FormData(this);
          fetch("/admin/newUpdate", {
            method: "POST",
            headers: { authorization: authCookie },
            body: formData,
          })
            .then((response) => response.json())
            .then((data) => {
              if (data.success) {
                addUpdateWarning.innerText = data.success;
                addUpdateForm.reset();
              } else if (data.error) {
                addUpdateWarning.innerText = data.error;
              }
            })
            .catch((error) => {
              console.error("Error:", error);
            });
        });

        const changeUpdateForm = document.getElementById("changeUpdateForm");
        const changeUpdateWarning = document.getElementById(
          "changeUpdateWarning"
        );
        changeUpdateForm.addEventListener("submit", function (event) {
          event.preventDefault();
          const formData = new FormData(this);
          fetch("/admin/changeUpdate", {
            method: "POST",
            headers: { authorization: authCookie },
            body: formData,
          })
            .then((response) => response.json())
            .then((data) => {
              if (data.success) {
                changeUpdateWarning.innerText = data.success;
                addUpdateForm.reset();
              } else if (data.error) {
                changeUpdateWarning.innerText = data.error;
              }
            })
            .catch((error) => {
              console.error("Error:", error);
            });
        });

        const deleteUpdateForm = document.getElementById("deleteUpdateForm");
        const deleteUpdateWarning = document.getElementById(
          "deleteUpdateWarning"
        );
        deleteUpdateForm.addEventListener("submit", function (event) {
          event.preventDefault();
          const formData = new FormData(this);
          fetch("/admin/deleteUpdate", {
            method: "POST",
            headers: { authorization: authCookie },
            body: formData,
          })
            .then((response) => response.json())
            .then((data) => {
              if (data.success) {
                deleteUpdateWarning.innerText = data.success;
                addUpdateForm.reset();
              } else if (data.error) {
                deleteUpdateWarning.innerText = data.error;
              }
            })
            .catch((error) => {
              console.error("Error:", error);
            });
        });
      });
    </script>
