    <script>
      function setCookie(name, value, days) {
        let date = new Date();
        date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000);
        let expires = "expires=" + date.toUTCString();
        document.cookie = name + "=" + value + ";" + expires;
      }
      function getCookie(name) {
        var value = null;
        document.cookie.split(";").map((element) => {
          var arr = element.split("=");
          if (arr[0] === ` ${name}` || arr[0] === name) {
            value = arr[1] === "" ? null : arr[1];
          }
        });
        return value;
      }
      var questionsSection;
      document.addEventListener("DOMContentLoaded", function () {
        const signinWarning = document.getElementById("signin_warning");
        const signinForms = document.getElementById("admin_signin");
        questionsSection = document.getElementById("questions");

        authorization = getCookie("authorization");

        if (!authorization) {
          signinForms.classList.add("disappear");
          signinWarning.innerText = "you don't have admin access!";
        } else {
          signinWarning.classList.add("disappear");
        }

        function insertQuestion(root, question, index) {
          var questionElement = document.createElement("div");
          questionElement.className = "questionCover";
          questionElement.id = `${index}testQuestion`;

          var queChild = document.createElement("div");
          queChild.innerText = `Q: ${index + 1}. ${question.question}`;
          queChild.className = "question";

          var optionsCover = document.createElement("div");
          optionsCover.className = "optionsCover";

          var optionA = document.createElement("div");
          optionA.innerText = `A: ${question.options.A}`;
          var optionB = document.createElement("div");
          optionB.innerText = `B: ${question.options.B}`;
          var optionC = document.createElement("div");
          optionC.innerText = `C: ${question.options.C}`;
          var optionD = document.createElement("div");
          optionD.innerText = `D: ${question.options.D}`;

          optionsCover.appendChild(optionA);
          optionsCover.appendChild(optionB);
          optionsCover.appendChild(optionC);
          optionsCover.appendChild(optionD);

          var answer = document.createElement("div");
          answer.innerText = `Answer: ${question.answer}`;
          answer.className = "answer";

          var removeBtn = document.createElement("button");
          removeBtn.className = "register-btn btn btn-primary btn-md mt-4";
          removeBtn.innerText = "remove";
          removeBtn.addEventListener("click", () => {
            removeQuestion(index);
          });

          questionElement.appendChild(queChild);
          questionElement.appendChild(optionsCover);
          questionElement.appendChild(answer);
          questionElement.appendChild(removeBtn);

          root.appendChild(questionElement);
        }

        function removeQuestion(index) {
          var questions = localStorage.getItem("questions");
          questions = JSON.parse(questions);
          questions = questions.filter((_, i) => i !== index);
          localStorage.setItem("questions", JSON.stringify(questions));
          document.getElementById(`${index}testQuestion`).remove();
        }

        var testData = localStorage.getItem("testData");
        if (testData) {
          testData = JSON.parse(testData);
          Object.entries(testData).map(([key, value]) => {
            console.log(key, value);
            document.getElementById(key).value = value;
          });
        }
        var questions = localStorage.getItem("questions");

        if (questions) {
          questions = JSON.parse(questions);
          questions.map((question, index) => {
            insertQuestion(questionsSection, question, index);
          });
        }
        const questionsForm = document.getElementById("questionsForm");
        const addOTWarning = document.getElementById("addOTWarning");

        questionsForm.addEventListener("submit", function (event) {
          event.preventDefault();
          const formData = new FormData(this);
          var question = {
            question: formData.get("question"),
            options: {
              A: formData.get("A"),
              B: formData.get("B"),
              C: formData.get("C"),
              D: formData.get("D"),
            },
            answer: formData.get("answer"),
          };
          var questions = localStorage.getItem("questions");
          if (!questions) {
            localStorage.setItem("questions", JSON.stringify([question]));
            insertQuestion(questionsSection, question, 0);
            questionsForm.reset();
          } else {
            questions = JSON.parse(questions);
            insertQuestion(questionsSection, question, questions.length);
            questions.push(question);
            localStorage.setItem("questions", JSON.stringify(questions));
            questionsForm.reset();
          }
        });

        const addOTForm = document.getElementById("addOTForm");
        addOTForm.addEventListener("submit", function (event) {
          event.preventDefault();
          const formData = new FormData(this);

          data = {
            subject: formData.get("subject"),
            chapter: formData.get("chapter"),
            standard: formData.get("standard"),
            testName: formData.get("testName"),
            teacher: formData.get("teacher"),
            startTime: formData.get("startTime"),
            endTime: formData.get("endTime"),
          };
          data = JSON.stringify(data);
          console.log(data);
          localStorage.setItem("testData", data);
        });
      });

      function handleAddOpenTest() {
        var data = localStorage.getItem("testData");

        data = JSON.parse(data);
        console.log(data);
        if (!data) {
          addOTWarning.innerText = "You needs to Click Add Details first!";
          return;
        }

        var questions = localStorage.getItem("questions");
        questions = JSON.parse(questions);
        if (!questions || questions.length === 0) {
          addOTWarning.innerText = "You needs to add at least one question!";
          return;
        }
        data.questions = JSON.stringify(questions);

        const startTime = new Date(data.startTime);
        const endTime = new Date(data.endTime);

        data.startTime = startTime.getTime();
        data.endTime = endTime.getTime();
        console.log(data);
        const formData = new FormData();
        Object.entries(data).forEach(([key, value]) => {
          formData.append(key, value);
        });

        fetch("/opentest/admin/addopentest", {
          method: "POST",
          headers: { authorization: authorization },
          body: formData,
        })
          .then((response) => response.json())
          .then((data) => {
            if (data.success) {
              addOTWarning.innerText = data.success;
              questionsForm.reset();
              addOTForm.reset();
              localStorage.clear();
              questionsSection.innerHTML = "";
            } else if (data.error) {
              addOTWarning.innerText = data.error;
            }
          })
          .catch((error) => {
            console.error("Error:", error);
          });
      }
      function handleResetDetails() {
        localStorage.removeItem("testData");
        addOTForm.reset();
      }
    </script>
